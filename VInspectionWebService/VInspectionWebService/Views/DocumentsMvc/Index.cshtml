@model IEnumerable<VInspectionWebService.Models.Document>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Placa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaSOAT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaRevTecnica)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Placa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaSOAT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaRevTecnica)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdDocumento }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdDocumento }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdDocumento })
        </td>
    </tr>
}

</table>
